<h1>All Transactions for <%= @group.name %></h1>

<button><%= link_to 'Add Transaction', new_group_entity_path(@group) %></button>

<p>Total Sum: <%= @entity_sum %></p>

<% Entity.show_trans.each do |ent| %>
        <div>
            <span><%= ent.name %></span>
            <span><%= ent.amount %></span>
            <span><%= ent.created_at %></span>
        </div> 
<% end %>

<%= link_to 'Back', groups_path, class: "btn btn-secondary" %>